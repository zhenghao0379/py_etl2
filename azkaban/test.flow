---
config:
  ws: cd /data/py_etl2/
  ws2: cd /data/py_etl2/ && source v
  ws3: cd /data/py_etl2/ && source ./venv/Scripts/activate && python3
  cwd: /data/py_etl2/
  DAYS: ${D}
  START: ${S}
  END: ${E}
  RPT_TPYE: ${T}
  MAIL: ${M}
  commend.1: bash ./pass_parameters.sh DAYS START END RPT_TYPE MAIL # 处理整合接收参数
  # command: cd /data/py_etl2 # 进入工作目录
  command.2: cd /data/py_etl2 && source ./venv/Scripts/activate # 启动虚拟环境

nodes:
  - name: root
    type: command
    config:
      command: pwd

  - name: test
    type: command
    dependsOn:
      - root
    config:
      command.1: echo ${config:py_flow_val}

  - name: test1
    type: command
    dependsOn:
      - root
    config:
      command.1: cd /data/demo
      command.2: python3 test.py

  - name: python
    type: command
    dependsOn:
      - root
    config:
      command: python3 /data/demo/test.py

  - name: python1
    type: command
    dependsOn:
      - root
    config:
      command: ${ws2} python3 demo/code/test2.py

  - name: python2
    type: command
    dependsOn:
      - root
    config:
      command: python3 demo/code/test2.py

  - name: python3
    type: command
    dependsOn:
      - root
    config:
      command: ${ws3} demo/code/test2.py
